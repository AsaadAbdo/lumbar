{
  "platforms": [ "iphone", "web" ],
  "packages": {
    "web": {
      "platforms": [ "web", "iphone" ]
    },
    "native": {
      "platforms": [ "iphone" ],
      "modules": [ "base", "home" ],
      "combine": true
    }
  },
  "modules": {
    "base": {
      "scripts": [
        {"style-map": true}
      ],
      "styles": [
        "styles/base.css"
      ]
    },
    "home": {
      "routes": {
        "": "home",
        "home": "home"
      },
      "styles": [
        "styles/home.css"
      ]
    }
  },

  "application": {
    "module": "base"
  },
  "styles": {
    "pixelDensity": {
      "iphone": [ 1, 2 ]
    }
  }
}
